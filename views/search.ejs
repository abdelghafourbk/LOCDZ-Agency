<%- include('header') -%>

<h1 class="pb-4">Search Results</h1>

<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">

    <% if(vehicule!= ''){ %>
        <% vehicule.forEach(function(vehicule, index) { %>            
    <a href="/vehicule/<%- vehicule._id %>" class="col text-center category__link">
        <div class="category__img category__img--large shadow">
            <img src="assets/<%- vehicule.carImage %>" alt=" <%- vehicule.model %> <%- vehicule.name %> " loading="lazy">
        </div>
        <div class="pt-1"> <%- vehicule.model %> <%- vehicule.name %></div>
    </a>
    <% }) %>
    <% }else{ %>
        <p>No Results, Try another thing!</p>
        <%}%>
</div>


<%- include('footer') -%>